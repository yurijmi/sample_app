<% provide(:title, "Edit user") %>
<div class="center">
<h1>Update your profile</h1>
</div>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
        <%= render 'fields', f: f %>

        <%= f.submit "Save changes", class: "btn btn-lg btn-primary" %>
    <% end %>
  </div>
</div>